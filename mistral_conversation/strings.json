{
  "config": {
    "abort": {
      "already_configured": "[%key:common::config_flow::abort::already_configured_service%]"
    },
    "error": {
      "cannot_connect": "[%key:common::config_flow::error::cannot_connect%]",
      "invalid_auth": "[%key:common::config_flow::error::invalid_auth%]",
      "unknown": "[%key:common::config_flow::error::unknown%]"
    },
    "step": {
      "user": {
        "data": {
          "api_key": "[%key:common::config_flow::data::api_key%]"
        }
      }
    }
  },
  "config_subentries": {
    "ai_task_data": {
      "abort": {
        "entry_not_loaded": "Cannot change this entry while it is disabled.",
        "reconfigure_successful": "[%key:common::config_flow::abort::reconfigure_successful%]"
      },
      "entry_type": "AI task",
      "error": {
        "model_not_supported": "This model is not supported, please select a different model"
      },
      "initiate_flow": {
        "reconfigure": "Reconfigure AI task",
        "user": "Add AI task"
      },
      "step": {
        "advanced": {
          "data": {
            "chat_model": "[%key:common::generic::model%]",
            "max_tokens": "Maximum tokens to return in response",
            "temperature": "Temperature",
            "top_p": "Top P",
            "reasoning_effort": "Reasoning effort"
          },
          "title": "Advanced settings"
        },
        "init": {
          "data": {
            "name": "[%key:common::config_flow::data::name%]",
            "recommended": "Recommended model settings"
          }
        }
      }
    },
    "conversation": {
      "abort": {
        "entry_not_loaded": "Cannot add items while the configuration is disabled.",
        "reconfigure_successful": "[%key:common::config_flow::abort::reconfigure_successful%]"
      },
      "entry_type": "Conversation agent",
      "error": {
        "model_not_supported": "This model is not supported, please select a different model"
      },
      "initiate_flow": {
        "reconfigure": "Reconfigure conversation agent",
        "user": "Add conversation agent"
      },
      "step": {
        "advanced": {
          "data": {
            "chat_model": "[%key:common::generic::model%]",
            "max_tokens": "Maximum tokens to return in response",
            "temperature": "Temperature",
            "top_p": "Top P"
          },
          "title": "Advanced settings"
        },
        "init": {
          "data": {
            "llm_hass_api": "[%key:common::config_flow::data::llm_hass_api%]",
            "name": "[%key:common::config_flow::data::name%]",
            "prompt": "[%key:common::config_flow::data::prompt%]",
            "recommended": "Recommended model settings"
          },
          "data_description": {
            "prompt": "Instruct how the LLM should respond. This can be a template."
          }
        }
      }
    }
  },
  "exceptions": {
    "invalid_config_entry": {
      "message": "Invalid config entry provided. Got {config_entry}"
    }
  },
  "services": {
    "generate_content": {
      "description": "Send a prompt to Mistral and return the textual response.",
      "fields": {
        "config_entry": {
          "description": "The config entry to use for this action",
          "name": "Config entry"
        },
        "filenames": {
          "description": "List of files to upload",
          "name": "Files"
        },
        "prompt": {
          "description": "The prompt to send",
          "name": "Prompt"
        }
      },
      "name": "Generate content"
    }
  }
}
